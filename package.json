{
  "name": "quickpath",
  "displayName": "QuickPath",
  "publisher": "pranto-paul",
  "description": "Create files and folders with just the keyboard",
  "version": "0.0.2",
  "icon": "logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Pranto-Paul/QuickPath"
  },
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
  "commands": [
    {
      "command": "quickpath.createFileOrDir",
      "title": "QuickPath: Create File/Dir"
    }
  ],
  "keybindings": [
    {
      "command": "quickpath.createFileOrDir",
      "key": "ctrl+alt+n",
      "when": "editorTextFocus"
    }
  ]
}
,
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.99.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.23.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
